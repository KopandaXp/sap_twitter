{
	"_version": "1.5.0",
	"sap.app": {
		"id": "sapTwitter",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"config": {
			"fullWidth": true
		},
		"rootView": {
			"viewName": "sapTwitter.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapTwitter.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "style/main.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "view",
				"controlId": "app",
				"controlAggregation": "pages",
				"clearAggregation": true,
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": ["appHeader", "myPage"]
				}
			],
			"targets": {
				"notFound": {
					"viewName": "NotFound"
				},
				"headerOverview": {
					"viewName": "HeaderOverview"
				},
				"appHeader": {
					"viewName": "AppHeader",
					"parent": "headerOverview",
					"controlId": "idHeaderOverviewPage",
					"controlAggregation": "fixContent"
				},
				"myPage": {
					"viewName": "MyPage",
					"parent": "headerOverview",
					"controlId": "idHeaderOverviewPage",
					"controlAggregation": "flexContent"
				}
			}
		}
	}
}